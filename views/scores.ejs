<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Scores</title>
  </head>
  <body>
    <div class="container">
      <h1>High Scores</h1>

      <table>
        <tr>
          <th>Name</th>
          <th>Score</th>
        </tr>
        <% games.forEach(function(game){ %>
          <tr>
            <td><a href='/games/<%=blog.id %>'><%= game.name %></a></td>
            <td><%= game.id %></td>
            <td><%= game.score %></td>
          </tr>
        <% }); %>
      </table>
      <a href="/games/game">Retry</a>
    </div>
  </body>
</html>
